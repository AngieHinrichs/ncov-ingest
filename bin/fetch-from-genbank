#!/bin/bash
set -euo pipefail

bin="$(dirname "$0")"

main() {
    fetch | "$bin"/csv-to-ndjson
}

fetch() {
    curl "$("$bin"/genbank-url)" \
        --fail --silent --show-error --http1.1 \
        --header 'User-Agent: https://github.com/nextstrain/ncov-ingest (hello@nextstrain.org)'
}

main "$@"
